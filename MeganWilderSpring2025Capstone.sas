/* Merging datasets ds4 and ds5 (ds4 contains info on people who reported that they experienced a crime, 
ds5 contains futher demographic information, etc. */
proc sort data=CAPSTONE.ds4 out=testds4;
by idper v4012; * sort by id variable, then sort by incident number;
run;

proc sort data=CAPSTONE.ds5 out=testds5;
by idper v4012;
run;

data capstone.merged;
merge testds4 testds5;
by idper;
run;	
	
/* Creating binary reported variable */
data capstone.merged;
	set capstone.merged;
	if V4399 = 1 then reported = "Reported";
	else if V4399 = 2 then reported = "Not Reported";
	else delete;
	

/* Creating binary variable for crimes against persons vs crimes against property */
data capstone.merged;
	set capstone.merged;
	if V4529 <= 23 then crimetype = "Property";
	else if V4529 >= 31 then crimetype = "Persons";
	else crimetype = .;
	
/* Creating variable for crimes against persons vs crimes against property */
data capstone.merged;
	set capstone.merged;
	if V4529 <= 20 then crimetype3 = "Property w/out personal";
	else if V4529 <= 30 then crimetype3 = "Property w/ personal";
	else if V4529 >= 31 then crimetype3 = "Persons";
	else crimetype3 = .;



/* Chi-square Test of Independence for crimetype vs reported */
PROC FREQ data=capstone.merged;
tables crimetype*reported / chisq relrisk expected cellchi2 or;
RUN;


/* Chi-square Test of Independence for crimetype3 vs reported */
PROC FREQ data=capstone.merged;
tables crimetype3*reported / chisq expected cellchi2;
RUN;


